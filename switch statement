var sign = prompt("what is your astronomical sign?");


switch(sign){
  case "Taurus":
    alert("gucci!");
    break;
  case "Libra":
    alert("versace");
    break;
  case "Leo":
    alert("hey there");
    break;
  default:
    alert("pick another weapon");
    break;
}

