var coffeeFlavor = "espresso";
var coffeeTemp = "piping hot";
var coffeeOunces = 100;
var coffeeMilk = true;

var myCoffee = {
  Flavor: "espresso",
  Temp: "piping hot",
  Ounces: 100,
  Milk: true,
  
  reheat: function(){
  if (myCoffee.Temp === "cold"){
   myCoffee.Temp = "piping hot";
     alert("your coffee is hot  again");
   }else{}
  }
};

